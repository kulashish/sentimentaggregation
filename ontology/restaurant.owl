   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#vegetarian"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	  	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#spicy"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	  <rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#GMOFree"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	  <rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#numberOfPeople"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>


  <owl:DatatypeProperty rdf:ID="vegetarian">
   	<rdfs:label>vegetarian</rdfs:label>
   	<rdfs:comment> property indicating if a dish is vegetarian or not</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="spicy">
   	<rdfs:label>spicy</rdfs:label>
   	<rdfs:comment> property indicating if a dish is spicy or not</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="GMOFree">
