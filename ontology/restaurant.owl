  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="mainChoice">
        <rdfs:label>mainChoice</rdfs:label>
        <rdfs:comment> property indicating the choice of main plates for a Meal</rdfs:comment>
	<rdfs:range rdf:resource="#Dish"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="desertOrCheeseChoice">
   	<rdfs:label>desertOrCheeseChoice</rdfs:label>
   	<rdfs:comment> property indicating the choice of desert in a Meal</rdfs:comment>
   	<rdfs:range rdf:resource="#Dish"/>
  </owl:ObjectProperty>  
  <owl:ObjectProperty rdf:ID="beverageChoice">
   	<rdfs:label>beverageChoice
   	</rdfs:label>
   	<rdfs:comment> property indicating the beverages available in a particular Meal</rdfs:comment>
	<rdfs:range rdf:resource="#Beverage"/>
  </owl:ObjectProperty>
 
  <owl:Class rdf:ID="Beverage">
   	<rdfs:label>Beverage</rdfs:label>
	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1">
     			<owl:onProperty rdf:resource="#name"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
