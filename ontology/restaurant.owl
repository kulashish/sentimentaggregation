	<rdfs:domain rdf:resource="#Wine"/>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:ObjectProperty> 

  <owl:Class rdf:ID="Dish">
   	<rdfs:label>Dish</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="#Dish">
	<rdfs:comment> A dish is described by a category(starter, fish, meat, pizza, cheese, desert), a name, a description, a price and some particularities such as spicy dish.
	</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#name"/>
