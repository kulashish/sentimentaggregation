 <rdf:RDF 
   xml:base="http://gaia.fdi.ucm.es/ontologies/restaurant.owl"
   xmlns="http://gaia.fdi.ucm.es/ontologies/restaurant.owl#"
    xmlns:cal="http://gaia.fdi.ucm.es/ontologies/calendar.owl#"
    xmlns:price="http://gaia.fdi.ucm.es/ontologies/price.owl#"
    xmlns:address="http://gaia.fdi.ucm.es/ontologies/address.owl#"
    xmlns:contact="http://gaia.fdi.ucm.es/ontologies/contact-details.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#">

  <owl:Ontology rdf:about="">
   <rdfs:comment>Restaurants Ontology</rdfs:comment>

   <owl:versionInfo>1.0 -  Translated to OWL: 01/2006</owl:versionInfo>
  </owl:Ontology>

  
  <owl:Class rdf:ID="Facility">
   	<rdfs:label>Facility</rdfs:label>
   	<rdfs:comment> Concept of Facility for any establishement. This class is an abstract class</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:ID="Catering">
   	<rdfs:label>RestaurantFacility</rdfs:label>
   	<rdfs:comment> Concept of RestaurantFacility identifies facility offer by a restaurant. This class is an abstract class</rdfs:comment>
  </owl:Class>

  <owl:Class rdf:about="#Catering">
	   <rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#description"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   </owl:Class>

  <owl:Class rdf:ID="TakeAwayFacility">
   	<rdfs:label>TakeAwayFacility</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Catering"/>
   	</rdfs:subClassOf>
  </owl:Class>


  <owl:Class rdf:about="#TakeAwayFacility">
	<rdfs:comment>
	Facility proposed by a restaurant to take away food. For each restaurant an instance of this facility identifies the discount percentage gievn by the restaurant to take away food and also indicates the possibility of taking away food by car.  
	</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#discountPercentage"/>
     			<owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#driveIn"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>



  <owl:Class rdf:ID="HomeDelivery">
   	<rdfs:label>HomeDeliveryFacility</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Catering"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:about="#HomeDelivery">
	<rdfs:comment> Facility proposed by a restaurant for home delivery. Each restaurant having this facility must described a particular instance of this catering such as last hour purchase information, delivery delay of the restaurant, possibilities of on-line order and on-line payment.
	</rdfs:comment>
	 <rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1">
     			<owl:onProperty rdf:resource="#deliveryArea"/>     			
    		</owl:Restriction>
   	</rdfs:subClassOf>
     	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#lastHourPurchase"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#deliveryDelay"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#onLineOrder"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#onLinePayment"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="ALaCarte">
   	<rdfs:label>HomeDeliveryFacility</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Catering"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="SelfService">
   	<rdfs:label>HomeDeliveryFacility</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Catering"/>
   	</rdfs:subClassOf>
  </owl:Class>


  <owl:Class rdf:ID="ChildrenAndBabyFacility">
   	<rdfs:label>ChildrenAndBabyFacility</rdfs:label>
	<rdfs:comment> A restaurant can offer some categories of Children and baby facilities.</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="ChildrenGameArea">
   	<rdfs:label>ChildrenGameArea</rdfs:label>
	<rdfs:comment> A restaurant can provide games for children </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ChildrenAndBabyFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="BabyChair">
   	<rdfs:label>BabyChair</rdfs:label>
	<rdfs:comment> A restaurant can provide Baby chairs </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ChildrenAndBabyFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ChildrenMeal">
   	<rdfs:label>ChildrenMeal</rdfs:label>
	<rdfs:comment> A restaurant can provide Children meal </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ChildrenAndBabyFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="BabyLaundry">
   	<rdfs:label>BabyLaundry</rdfs:label>
	<rdfs:comment> A restaurant can provide a baby laundry </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ChildrenAndBabyFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="BabySitter">
   	<rdfs:label>BabySitter</rdfs:label>
	<rdfs:comment> A restaurant can provide a baby sitter </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ChildrenAndBabyFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="SmokingFacility">
   	<rdfs:label>SmokingFacility</rdfs:label>
   	<rdfs:comment>A restaurant can offer some categories of SmokingFacilities such as smoking area, smoking room etc.  </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="SmokingArea">
   	<rdfs:label>SmokingArea</rdfs:label>
   	<rdfs:comment>A restaurant can offer a smoking area facility </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#SmokingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="SmokingRoom">
   	<rdfs:label>SmokingRoom</rdfs:label>
   	<rdfs:comment>A restaurant can offer a smoking room facility </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#SmokingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="NoSmoking">
   	<rdfs:label>NoSmoking</rdfs:label>
   	<rdfs:comment>no-smoking facility for smoking facility </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#SmokingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:ID="Smoking">
   	<rdfs:label>Smoking</rdfs:label>
   	<rdfs:comment>no-smoking facility for smoking facility </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#SmokingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="ParkingFacility">
   	<rdfs:label>ParkingFacility</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:ID="IndoorParking">
   	<rdfs:label>ParkingIndoor</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ParkingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:ID="OutdoorParking">
   	<rdfs:label>ParkingOutdoor</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ParkingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:ID="PrivateParking">
   	<rdfs:label>PrivateParking</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ParkingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

<owl:Class rdf:ID="PublicParking">
   	<rdfs:label>PublicParking</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ParkingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:ID="ValetParking">
   	<rdfs:label>ValetParking</rdfs:label>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#ParkingFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PetFacility">
   	<rdfs:label>PetFacility</rdfs:label>
   	<rdfs:comment>An establishment can offer some categories of PetFacility concept such as Pet-accepted, pet-refused, only-dog-accepted. </rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="OnlyDogAccepted">
   	<rdfs:label>OnlyDogAccepted</rdfs:label>
   	<rdfs:comment>An establishment can offer an only dog accepted pet facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PetFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PetAccepted">
   	<rdfs:label>PetAccepted</rdfs:label>
   	<rdfs:comment>An establishment can offer a pet accepted pet facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PetFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PetRefused">
   	<rdfs:label>PetRefused</rdfs:label>
   	<rdfs:comment>An establishment can refused pet as pet facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PetFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PetFood">
   	<rdfs:label>PetFood</rdfs:label>
   	<rdfs:comment>An establishment can have a pet food facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PetFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="PetNursery">
   	<rdfs:label>PetRefused</rdfs:label>
   	<rdfs:comment>An establishment can have a pet nursery</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PetFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="DisabledFacility">
   	<rdfs:label>DisabledFacility</rdfs:label>
   	<rdfs:comment>>An establishement can offer some categories of Disabled facility such as  wheel chair access, elevator</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="WheelChairAccess">
   	<rdfs:label>WheelChairAccess</rdfs:label>
   	<rdfs:comment>>An establishement can offer some generic instance of the Disabled concept such as  wheel chair access, elevator</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#DisabledFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="Elevator">
   	<rdfs:label>Elevator</rdfs:label>
   	<rdfs:comment>>An establishement can offer some generic instance of the Disabled concept such as  wheel chair access, elevator</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#DisabledFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="WheelChairToilet">
   	<rdfs:label>Elevator</rdfs:label>
   	<rdfs:comment>>An establishement can offer a wheelChairToilet facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#DisabledFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="BrailleMenu">
   	<rdfs:label>Elevator</rdfs:label>
   	<rdfs:comment>>An establishement can a brailleMenu facility</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#DisabledFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="PaymentFacility">
   	<rdfs:label>PaymentFacility</rdfs:label>
   	<rdfs:comment>>An establishement can offer particular payment facilities</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="CBAccepted">
   	<rdfs:label>CBAccepted</rdfs:label>
   	<rdfs:comment>>An establishement can offer CB accepted payement facility: see to introduce type of card or list of predefined instance of payment card</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#PaymentFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

 <owl:Class rdf:about="#CBAccepted">
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1">
     			<owl:onProperty rdf:resource="#cardType"/>
     			<owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>
  
   <owl:Class rdf:ID="BusinessFacility">
   	<rdfs:label>BusinessFacility</rdfs:label>
   	<rdfs:comment>>A restaurant  can offer particular business restaurant facilities such as business lunch, breakfast</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="BusinessBreakfast">
   	<rdfs:label>BusinessBreakfast</rdfs:label>
   	<rdfs:comment>>A restaurant can propose business breakfast</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#BusinessFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="BusinessLunch">
   	<rdfs:label>BusinessFacility</rdfs:label>
   	<rdfs:comment>>A restaurant can propose business lunch</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#BusinessFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

<owl:Class rdf:ID="BusinessDining">
	<rdfs:comment>A restaurant can propose business dining</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#BusinessFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>

   <owl:Class rdf:ID="AccessFacility">
   	<rdfs:label>AccessFacility</rdfs:label>
   	<rdfs:comment>>An establishement can be closed to a metro station, taxi station or any other transport facilities</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#Facility"/>
   	</rdfs:subClassOf>
  </owl:Class>


   <owl:Class rdf:ID="MetroFacility">
   	<rdfs:label>MetroFacility</rdfs:label>
	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0">
     			<owl:onProperty rdf:resource="#stationName"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Class rdf:about="#AccessFacility"/>
   	</rdfs:subClassOf>
  </owl:Class>
  
  <owl:DatatypeProperty rdf:ID="description">
   	<rdfs:label>description
   	</rdfs:label>
   	<rdfs:comment>generic property or adding a description to any objects</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="discountPercentage">
   	<rdfs:label>discountPercentage</rdfs:label>
   	<rdfs:comment>property for a take awayfacility indicating the discount percentage offer when people take away their food </rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#TakeAwayFacility">
    		</owl:Class>
   	</rdfs:domain>
   	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="driveIn">
   	<rdfs:label>driveIn
   	</rdfs:label>
   	<rdfs:comment> property indicating if there is a driveIn for the take-away facility</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#TakeAwayFacility">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>  
  
    <owl:DatatypeProperty rdf:ID="deliveryArea">
   	<rdfs:label>deliveryArea</rdfs:label>
   	<rdfs:comment> property indicating the area for the delivery in the case of a home delivery facility</rdfs:comment>
	<rdfs:domain>
    		<owl:Class rdf:about="#HomeDelivery">
    		</owl:Class>
   	</rdfs:domain>
   	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  
  
  <owl:DatatypeProperty rdf:ID="lastHourPurchase">
   	<rdfs:label>lastHourPurchase
   	</rdfs:label>
   	<rdfs:comment> property indicating the last hour to purchase something from a restaurant having a HomeDeliveryFacility</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#HomeDelivery">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/calendar.owl#Time"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="deliveryDelay">
   	<rdfs:label>deliveryDelay</rdfs:label>
   	<rdfs:comment> property indicating the delay for the delivery in the case of a home delivery facility</rdfs:comment>
	<rdfs:domain>
    		<owl:Class rdf:about="#HomeDelivery">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/calendar.owl#Duration"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="onLineOrder">
   	<rdfs:label>onLineOrder</rdfs:label>
   	<rdfs:comment> boolean indicating if the HomeDeliveryFacility has the possibility of on-line order</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#HomeDelivery">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="onLinePayment">
   	<rdfs:label>onLinePayment</rdfs:label>
   	<rdfs:comment> property indicating if an on-line payment is possible for the HomeDeliveryFacility of a restaurant</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#HomeDelivery">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
  </owl:DatatypeProperty>
    
  <owl:DatatypeProperty rdf:ID="cardType">
   	<rdfs:label>cardType</rdfs:label>
   	<rdfs:comment> property indicating the type of payment card</rdfs:comment>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="stationName">
   	<rdfs:label>stationName</rdfs:label>
   	<rdfs:domain>
    		<owl:Class rdf:about="#MetroFacility" />
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  
  
  
  
  
 <!--Classes related to restaurant constraints such as period of closing, opening-days and opening-hours-->

  <owl:Class rdf:ID="DatePeriod">
   	<rdfs:label>DatePeriod</rdfs:label>
   	<rdfs:comment> A period is set of days determined by a startPeriodDate and a endPeriodDate</rdfs:comment>
	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#startPeriodDate"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#endPeriodDate"/>
