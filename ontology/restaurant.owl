   <owl:Class rdf:ID="OpeningPeriod">
   	<rdfs:label>OpeningPeriod</rdfs:label>
   </owl:Class>


   <owl:Class rdf:about="#OpeningPeriod">
	<rdfs:comment> This concept summarizes the list of days which have the same opening hours</rdfs:comment>
	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1">
     			<owl:onProperty rdf:resource="#days"/>
		</owl:Restriction>
   	</rdfs:subClassOf>
	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1" owl:maxCardinality="2">
     			<owl:onProperty rdf:resource="#periods"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:ObjectProperty rdf:ID="days">
   	<rdfs:label>days</rdfs:label>
   	<rdfs:comment> property indicating the list of opening days in the concept OpeningPeriod</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#OpeningPeriod">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/calendar.owl#DayOfWeek"/>
  </owl:ObjectProperty>
  
   <owl:ObjectProperty rdf:ID="periods">
   	<rdfs:label>periods</rdfs:label>
   	<rdfs:comment> property indicating a time-slot of periods</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#OpeningPeriod">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="#TimePeriod"/>
  </owl:ObjectProperty>

   <owl:Class rdf:ID="TimePeriod">
	<rdfs:label>TimePeriod</rdfs:label>
   </owl:Class>	

   <owl:Class rdf:about="#TimePeriod">
	<rdfs:comment>
		This class allows to describe a time slot.
	</rdfs:comment>
	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#startHour"/>
    		</owl:Restriction>
	</rdfs:subClassOf>
	<rdfs:subClassOf>
		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#stopHour"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   </owl:Class>
 
  <owl:DatatypeProperty rdf:ID="startHour">
   	<rdfs:label>startHour</rdfs:label>
   	<rdfs:comment> property indicating the start time for the time-slot of TimePeriod</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#TimePeriod">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/calendar.owl#Time"/>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:ID="stopHour">
   	<rdfs:label>stopHour</rdfs:label>
   	<rdfs:comment> property indicating stop time for the time-slot of TimePeriod</rdfs:comment>
   	<rdfs:domain>
    		<owl:Class rdf:about="#TimePeriod">
    		</owl:Class>
   	</rdfs:domain>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/calendar.owl#Time"/>
  </owl:DatatypeProperty>
  






<!--Classes for Meals, dishes, beverages-->

   <owl:Class rdf:ID="Meal">
   	<rdfs:label>Meal</rdfs:label>
   </owl:Class>

   <owl:Class rdf:about="#Meal">
	<rdfs:comment>This class is an abstract class. Each type of Meal is a subClass of this Concept.</rdfs:comment>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0" owl:maxCardinality="1">
     			<owl:onProperty rdf:resource="#name"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#price"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	<rdfs:subClassOf>
    		<owl:Restriction owl:cardinality="1">
     			<owl:onProperty rdf:resource="#numberOfPeople"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0">
     			<owl:onProperty rdf:resource="#starterChoice"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="1">
     			<owl:onProperty rdf:resource="#mainChoice"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0">
     			<owl:onProperty rdf:resource="#desertOrCheeseChoice"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
   	<rdfs:subClassOf>
    		<owl:Restriction owl:minCardinality="0">
     			<owl:onProperty rdf:resource="#beverageChoice"/>
    		</owl:Restriction>
   	</rdfs:subClassOf>
  </owl:Class>

  <owl:DatatypeProperty rdf:ID="name">
   	<rdfs:label>name</rdfs:label>
   	<rdfs:comment> generic property for name</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:ID="price">
   	<rdfs:label>price</rdfs:label>
   	<rdfs:comment> generic property indicating a price</rdfs:comment>
	<rdfs:range rdf:resource="http://gaia.fdi.ucm.es/ontologies/price.owl#Price"/>
  </owl:DatatypeProperty>
   <owl:DatatypeProperty rdf:ID="numberOfPeople">
   	<rdfs:label>numberOfPeople</rdfs:label>
   	<rdfs:comment> generic property indicating a number of people</rdfs:comment>
	<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#NonNegativeInteger"/>
  </owl:DatatypeProperty>
  <owl:ObjectProperty rdf:ID="starterChoice">
        <rdfs:label>starterChoice</rdfs:label>
        <rdfs:comment> property indicating the choice of starters (Dish) for a Meal</rdfs:comment>
